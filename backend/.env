# Configuration du service AI pour le générateur de posts LinkedIn

# Fournisseur AI par défaut (claude uniquement)
DEFAULT_AI_PROVIDER=claude

# Clé API Claude (OBLIGATOIRE)
CLAUDE_API_KEY=

# Configuration du serveur
NODE_ENV=development
PORT=3000

# Options de génération de posts
MAX_POST_LENGTH=1300
TONE_OPTIONS=professionnel,casual,inspirant,informatif,humoristique,formel,storytelling

# Logging
LOG_LEVEL=info

# URLs et configurations supplémentaires
FRONTEND_URL=http://localhost:3000
